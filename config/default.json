{
  "log4js": {
    "level": "ALL",
    "configure": {
      "appenders": [
        {
          "category": "default",
          "type": "console"
        },
        {
          "category": "access",
          "type": "console"
        },
        {
          "category": "error",
          "type": "console"
        }
      ],
      "replaceConsole": true
    }
  }
}
